st=10e3
td=100
l=99

simdir=/projects/f_mlp195/brendanr/g${g}_n${nhist}_td${td}_l${l}_n500/

yes $g | head -n 120 > $simdir/gcol
yes $nhist | head -n 120 > $simdir/ncol
yes $td | head -n 120 > $simdir/tdcol
yes $l | head -n 120 > $simdir/lcol

nsamp=50
nloc=10k

LDoutfile=${simdir}rad${nloc}_n${nsamp}_LDNeout.txt

LDoutlong=${simdir}rad${nloc}_n${nsamp}_LDNeoutlong.txt

LDoutdats1=${simdir}rad${nloc}_n${nsamp}_LDNeoutdats.txt

awk '{print $2,$3,$4,$5,$6}' OFS="\n" ${LDoutfile} | tail -n 120 > ${LDoutlong}

paste $simdir/gcol $simdir/ncol $simdir/tdcol $simdir/lcol radn50col rad10kcol longtab_fields ${LDoutlong} > ${LDoutdats1}

